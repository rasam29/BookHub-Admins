<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rasam.bookhubadmins">

    <application
        android:allowBackup="true"
        android:name=".maintanance.App"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".splash.view.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".auth.view.RegisterActivity" />
        <activity android:name=".auth.fragments.reportVerify.view.ReportVerifyActivity" />
        <activity android:name=".auth.fragments.AuthKeyVerification.view.AuthKeyActivity" />
        <activity android:name=".auth.fragments.BlockedFragment" />
        <activity android:name=".main.view.MainActivity" />
        <activity android:name=".productInfo.view.MoreDetailsActivity" />

        <activity android:name=".historyManager.view.deleted.DeletedHistoryActivity" />
        <activity android:name=".historyManager.view.promoted.PromotedHistoryActivity" />
        <activity android:name=".contactUs.view.ContactUsActivity" />
    </application>

</manifest>